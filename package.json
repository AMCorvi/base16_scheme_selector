{
  "name": "base16_scheme_selector",
  "version": "0.0.0",
  "description": "A simple interactive cli tool for switching between base16 shell color themes.",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --watch --config webpack.config.js",
    "clean": "rimraf build",
    "run:watch": "nodemon -x 'npm run build; node build/bundled_main.js' -w src",
    "start:dev": "run-p test build:watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "AMCorvi <am@corvi.co>",
  "license": "MIT",
  "dependencies": {
    "inquirer": "6.2.0",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.5",
    "@babel/preset-env": "7.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "hard-source-webpack-plugin": "0.12.0",
    "jest": "23.6.0",
    "nodemon": "1.18.6",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}
